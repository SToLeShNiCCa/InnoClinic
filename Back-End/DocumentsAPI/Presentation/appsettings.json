{
  "DevelopmentStorage": {
    "AzureBlobStorage": "UseDevelopmentStorage=true"
  },
  "AzurePhotoDatabaseSettings": {
    "AzureBlobContainer": "photoes"
  },
  "DocumentBlobStorage": {
    "AzureBlobContainer": "documents"
  },
  "PhotosDatabaseSettings": {
    "ConnectionString": "mongodb://localhost:27017",
    "DatabaseName": "DocumentsAPI",
    "CollectionName": "Photoes"
  },
  "ResultsDatabaseSettings": {
    "ConnectionString": "mongodb://localhost:27017",
    "DatabaseName": "DocumentsAPI",
    "CollectionName": "Results"
  },
  "DocumentsDatabaseSettings": {
    "ConnectionString": "mongodb://localhost:27017",
    "DatabaseName": "DocumentsAPI",
    "CollectionName": "Documents"
  },
  "KeyCloak": {
    "AuthorizationUrl": "http://localhost:8080/realms/KeyCloackAuth/protocol/openid-connect/auth"
  },
  "Authentication": {
    "MetadataAddress": "http://localhost:8080/realms/KeyCloackAuth/.well-known/openid-configuration",
    "ValidIssuer": "http://localhost:8080/realms/KeyCloackAuth",
    "Audience": "account"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  }
}